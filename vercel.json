{
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
        },
        {
            "source": "/(.*)",
            "headers": [
                { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
                { "key": "X-Content-Type-Options", "value": "nosniff" },
                { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/",
            "destination": "/index.html"
        },
        {
            "source": "/about",
            "destination": "/about.html"
        },
        {
            "source": "/discord",
            "destination": "/discord.html"
        },
        {
            "source": "/lootlabs",
            "destination": "/lootlabs.html"
        },
        {
            "source": "/privacy",
            "destination": "/privacy.html"
        },
        {
            "source": "/projects",
            "destination": "/projects.html"
        },
        {
            "source": "/projects/aplok-guns",
            "destination": "/projects/aplok-guns.html"
        },
        {
            "source": "/projects/apocalyptic-clothes",
            "destination": "/projects/apocalyptic-clothes.html"
        },
        {
            "source": "/projects/dayz-legacy-map",
            "destination": "/projects/dayz-legacy-map.html"
        },
        {
            "source": "/projects/mini-apocalypse",
            "destination": "/projects/mini-apocalypse.html"
        },
        {
            "source": "/projects/project-walker",
            "destination": "/projects/project-walker.html"
        },
        {
            "source": "/terms",
            "destination": "/terms.html"
        }
    ]
}
